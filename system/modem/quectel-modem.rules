# Symlink for Station Modem

# prevent modem manager from accessing modem serial ports
KERNEL=="ttyUSB[0-2]", \
	ATTRS{idVendor}=="2c7c", ATTRS{idProduct}=="0125", \
	ENV{ID_MM_DEVICE_IGNORE}="1"

# prevent modem manager from accessing adafruit microcontrollers to be safe
SUBSYSTEM=="tty", ACTION=="add", ATTRS{idVendor}="239a",ATTRS{idProduct}="800c", ENV{ID_MM_DEVICE_IGNORE}="1"

